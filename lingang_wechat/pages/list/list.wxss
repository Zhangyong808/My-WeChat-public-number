.list_container{
    background: #373a40;
}
.list_container>.imgTip{
    width: 100%;
    height: 307rpx;
}
.list_container>.imgTip>image{
    width: 100%;
    height: 100%;
}
.list_container .navList{
    padding: 10rpx 0rpx;
    text-indent: 30rpx;
    display: flex;
    align-items: center;
    padding: 30rpx 0rpx;
    /* color: #9e9fa2; */
    color: white;
    border: 1rpx solid #2d2f34;
    font-size: 30rpx;
}
.list_container .navList>image{
    width: 41rpx;
    height: 41rpx;
    margin: 0 40rpx 0rpx 30rpx;
}
#groupAll{
    position: relative;
}
#groupAll>.arrow1{
    width: 15rpx;
    height: 20rpx;
    position: absolute;
    right: 20rpx;
    top: 50%;
    transform: translateY(-50%);
}
#groupAll>.arrow2{
    width: 20rpx;
    height: 15rpx;
    position: absolute;
    right: 20rpx;
    top: 50%;
    transform: translateY(-50%);
}
.group>.groupList{
    padding: 5rpx 0rpx;
    text-indent: 110rpx;
    display: flex;
    align-items: center;
    padding: 30rpx 0rpx;
    /* color: #9e9fa2; */
    color: white;
    border: 1rpx solid #2d2f34;
    font-size: 32rpx;
}
.group>.group_show{
    display: block;
}
.group>.group_hide{
    display: none;
}